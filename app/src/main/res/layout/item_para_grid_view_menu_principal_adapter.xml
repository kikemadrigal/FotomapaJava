<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginTop="20dp">

    <ImageView
        android:id="@+id/imageViewItemMenuPrincipalFragment"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:src="@drawable/imagencabecera" />

    <TextView
        android:id="@+id/textView1ItemMenuPrincipalFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:singleLine="true"
        android:text="texto de arriba" />

    <TextView
        android:id="@+id/textView2ItemMenuPrincipalFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:singleLine="true"
        android:text="texto central" />
    <!--<Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Ir las coordenadas"
        android:textSize="10dp"
        android:layout_below="@id/textView2ItemMenuPrincipalFragment"/>-->


</LinearLayout>